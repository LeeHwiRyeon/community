{
    "name": "auto-dev-system",
    "version": "1.0.0",
    "description": "자동 개발 시스템 - 목표만 입력하면 완전한 소프트웨어 자동 개발",
    "main": "dist/index.js",
    "scripts": {
        "dev": "tsx watch src/index.ts",
        "build": "tsc",
        "start": "node dist/index.js",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:coverage": "jest --coverage",
        "lint": "eslint src/**/*.ts",
        "lint:fix": "eslint src/**/*.ts --fix",
        "format": "prettier --write src/**/*.ts",
        "ai:train": "python scripts/train-models.py",
        "ai:predict": "python scripts/predict.py",
        "generate:project": "tsx src/generators/project-generator.ts",
        "generate:api": "tsx src/generators/api-generator.ts",
        "generate:ui": "tsx src/generators/ui-generator.ts",
        "test:auto": "tsx src/testing/test-runner.ts",
        "optimize:code": "tsx src/optimization/code-optimizer.ts",
        "optimize:db": "tsx src/optimization/database-optimizer.ts",
        "analyze:ux": "tsx src/ux/behavior-analyzer.ts",
        "track:bugs": "tsx src/bug-tracking/detector.ts",
        "orchestrate": "tsx src/core/orchestrator.ts"
    },
    "dependencies": {
        "@types/node": "^20.0.0",
        "typescript": "^5.0.0",
        "tsx": "^4.0.0",
        "express": "^4.18.0",
        "cors": "^2.8.5",
        "helmet": "^7.0.0",
        "morgan": "^1.10.0",
        "dotenv": "^16.0.0",
        "openai": "^4.0.0",
        "anthropic": "^0.3.0",
        "redis": "^4.6.0",
        "pg": "^8.11.0",
        "mongodb": "^6.0.0",
        "bullmq": "^4.0.0",
        "prom-client": "^15.0.0",
        "winston": "^3.10.0",
        "joi": "^17.9.0",
        "axios": "^1.5.0",
        "cheerio": "^1.0.0-rc.12",
        "playwright": "^1.40.0",
        "jest": "^29.7.0",
        "supertest": "^6.3.0",
        "k6": "^0.0.0",
        "lighthouse": "^11.0.0",
        "pa11y": "^8.0.0",
        "eslint": "^8.50.0",
        "prettier": "^3.0.0",
        "nodemon": "^3.0.0"
    },
    "devDependencies": {
        "@types/express": "^4.17.0",
        "@types/cors": "^2.8.0",
        "@types/morgan": "^1.9.0",
        "@types/jest": "^29.5.0",
        "@types/supertest": "^2.0.0",
        "@types/cheerio": "^0.22.0",
        "ts-jest": "^29.1.0"
    },
    "keywords": [
        "ai",
        "automation",
        "code-generation",
        "testing",
        "optimization",
        "ux",
        "bug-tracking"
    ],
    "author": "Auto Dev System",
    "license": "MIT"
}
