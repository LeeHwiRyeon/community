{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft": "Warning",
            "Microsoft.Hosting.Lifetime": "Information"
        }
    },
    "Agent": {
        "DefaultMaxSteps": 10,
        "DefaultMaxRunTimeSeconds": 120,
        "Policy": {
            "MaxRetries": 3,
            "BackoffMs": 2000,
            "MaxCharsPerStep": 200000
        },
        "StoragePath": "agent.db"
    },
    "Scheduler": {
        "RecurringWorkflows": [
            {
                "Objective": "커뮤니티 플랫폼 상태 모니터링",
                "PeriodMinutes": 2,
                "Priority": 1,
                "Context": {
                    "health_url": "http://localhost:5000/api/health-check",
                    "out": "artifacts/community/health-status.txt"
                },
                "Plan": [
                    {
                        "Order": 1,
                        "Tool": "http.get",
                        "Input": "%ctx:health_url%"
                    },
                    {
                        "Order": 2,
                        "Tool": "file.write",
                        "Input": "%ctx:out%"
                    },
                    {
                        "Order": 3,
                        "Tool": "webhook.post",
                        "Input": "http://localhost:5000/api/agent/status"
                    }
                ]
            },
            {
                "Objective": "커뮤니티 통계 수집",
                "PeriodMinutes": 5,
                "Priority": 2,
                "Context": {
                    "stats_url": "http://localhost:5000/api/stats",
                    "out": "artifacts/community/stats-%now:yyyyMMdd-HHmm%.txt"
                },
                "Plan": [
                    {
                        "Order": 1,
                        "Tool": "http.get",
                        "Input": "%ctx:stats_url%"
                    },
                    {
                        "Order": 2,
