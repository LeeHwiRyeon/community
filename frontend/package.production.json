{
    "name": "community-platform-frontend",
    "version": "1.2.0",
    "description": "Community Platform v1.2 - 차세대 혁신 CMS 플랫폼 프론트엔드",
    "type": "module",
    "scripts": {
        "dev": "vite --config vite.config.ts",
        "build": "vite build --config vite.config.production.ts",
        "build:analyze": "ANALYZE=true vite build --config vite.config.production.ts",
        "preview": "vite preview --config vite.config.production.ts",
        "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
        "lint:fix": "eslint . --ext ts,tsx --fix",
        "type-check": "tsc --noEmit",
        "test": "vitest",
        "test:ui": "vitest --ui",
        "test:coverage": "vitest --coverage",
        "test:e2e": "playwright test",
        "test:e2e:ui": "playwright test --ui",
        "format": "prettier --write .",
        "format:check": "prettier --check .",
        "clean": "rimraf dist node_modules/.vite",
        "clean:all": "rimraf dist node_modules/.vite node_modules",
        "reinstall": "npm run clean:all && npm install",
        "start": "npm run preview",
        "deploy": "npm run build && npm run preview",
        "deploy:staging": "npm run build && npm run preview -- --port 4173",
        "deploy:production": "npm run build && npm run preview -- --port 4173 --host",
        "health-check": "curl -f http://localhost:4173/health || exit 1",
        "performance-test": "lighthouse http://localhost:4173 --output html --output-path ./dist/lighthouse-report.html",
        "bundle-analyze": "npm run build:analyze",
        "security-audit": "npm audit --audit-level moderate",
        "dependencies-check": "npm outdated",
        "update-dependencies": "npm update",
        "postinstall": "npm run type-check",
        "prebuild": "npm run clean && npm run type-check",
        "postbuild": "npm run health-check"
    },
